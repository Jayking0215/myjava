package ex04;

import org.springframework.context.support.FileSystemXmlApplicationContext;

public class HelloAppSprint {

	public static void main(String[] args) {
		String config="src/main/java/ex04/appContext.xml";//설정파일 경로
		ApplicationContext ctx=new FileSystemXmlApplicationContext(config);//부모타입 & 자식객체
	}

}
