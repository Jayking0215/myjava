package ex04;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.FileSystemXmlApplicationContext;

public class HelloAppSprint {

	public static void main(String[] args) {
		String config="src/main/java/ex04/appContext.xml";//설정파일 경로
		ApplicationContext ctx=new FileSystemXmlApplicationContext(config);//스프링 컨테이너:부모타입 & 자식객체
		
		Message msg=ctx.getBean("mb2", Message.class);//Message.class로 형변환
		msg.sayHello();
		msg.sayHi("수지","철수","영희","BTS");
		
	}
}
